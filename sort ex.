#include <iostream>
using namespace std;

int main() {
    return 0;
}

const int SIZE = 5;
int ara[]={3,8,6,1,5};
int temp, value;
cout << "Original..\n";
for (int i=0; i < SIZE; i++)
    cout << ara[i] << " ";
cout << endl;
for (int i =SIZE-1; i>0; i--)
{
    value = 0;
    for(int j=0; j<1; j++)
    {
        if (ara[j] > ara[value])
        {
            value = j;
        }
    }
    temp = ara[value];
    ara[value]=ara[i];
    ara[i]=temp;
}

cout << "sorted..";
for (int i=0; i<SIZE; i++)
    cout << ara[i] << " ";
